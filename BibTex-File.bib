@article{10.2307/2010133,
 ISSN = {00438871, 10863338},
 URL = {http://www.jstor.org/stable/2010133},
 abstract = {The concept of a "strategic triangle" is useful in an analysis of the internal logic of the relationship between the United States, the Soviet Union, and China. The preconditions for a triangular relationship are that each player recognize the strategic salience of the three principals, and the relationship between any two will be affected by each player's relationship to the third. Within the triangle, there are three distinct pattern dynamics: the menage a trois, consisting of mutually positive relationships among all three; the stable marriage, consisting of a bilateral relationship excluding the third, and the romantic triangle, consisting of one pivot player playing off two suitors. Each of these pattern dynamics has specific rules of rational play. The shift from one pattern dynamic to another is a function of the attempts of the players to freeze a given configuration through commitment to a treaty or a common ideology, interacting with periodic crises that test their commitments.},
 author = {Lowell Dittmer},
 journal = {World Politics},
 number = {4},
 pages = {485-515},
 publisher = {Cambridge University Press},
 title = {The Strategic Triangle: An Elementary Game-Theoretical Analysis},
 volume = {33},
 year = {1981}
}




@article{Biresselioglu2014,
title = "Investigating The Natural Gas Supply Security: A New Perspective",
journal = "Energy",
volume = "",
number = "0",
pages = " - ",
year = "2014",
note = "",
issn = "0360-5442",
doi = "http://dx.doi.org/10.1016/j.energy.2014.11.060",
